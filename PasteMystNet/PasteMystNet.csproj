<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Title>PasteMyst.NET</Title>
        <Authors>Dennise Catolos</Authors>
        <Copyright>Copyright Â© 2020</Copyright>
        <PackageProjectUrl>https://dennise.me/projects/pastemystnet</PackageProjectUrl>
        <PackageLicenseUrl>https://dennise.me/license</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/dentolos19/PasteMyst.NET</RepositoryUrl>
        <SignAssembly>true</SignAssembly>
        <AssemblyOriginatorKeyFile>Properties\AssemblyKey.snk</AssemblyOriginatorKeyFile>
        <PublicSign>true</PublicSign>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    </ItemGroup>

</Project>
