<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>

		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>9.0</LangVersion>

		<PackageIcon>Icon.png</PackageIcon>
		<Version>1.1.7</Version>
		<Authors>Dennise Catolos</Authors>
		<Description>A simple .NET API wrapper for PasteMyst!</Description>
		<Copyright>Copyright Â© 2021</Copyright>

		<PackageProjectUrl>https://github.com/dentolos19/PasteMystNet</PackageProjectUrl>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>

		<RepositoryUrl>https://github.com/dentolos19/PasteMystNet</RepositoryUrl>
		<RepositoryUrl>git</RepositoryUrl>

	</PropertyGroup>

	<ItemGroup>
		<None Include="..\LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<Resource Include="Resources\Assets\Icon.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</Resource>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1"/>
	</ItemGroup>

</Project>